    <script type="text/javascript">
      $(document).on('ready', function () {
        $(document).foundation();
        {% if app.dtToken is defined %}
          gmwa.init('{{app.dtToken}}', true, {{mapFramework}});
        {% else %}
          gmwa.init(undefined, true, {{mapFramework}});
        {% endif %}
      });
    </script>
  {% else %}
    <script type="text/javascript">
      function downloadJSAtOnload() {
        var element = document.createElement("script");
        element.src = '{{ app.request.basepath }}/js/dist/vendors{{ filesSignature }}.js';
        element.onload = function(){
          $(document).foundation();
          var element = document.createElement("script");
          element.src = '{{ app.request.basepath }}/js/dist/app{{ filesSignature }}.js';
          element.onload = function(){    
            {% if app.dtToken is defined %}
              gmwa.init('{{app.dtToken}}', false, {{mapFramework}});
            {% else %}
              gmwa.init(undefined, false, {{mapFramework}});
            {% endif %}
          };
          document.body.appendChild(element);
        }
        document.body.appendChild(element);
      }
      document.addEventListener("DOMContentLoaded", downloadJSAtOnload);
    </script>
  {% endif %}
{% else %}
  <script type="text/javascript">
    document.addEventListener("DOMContentLoaded", function(){
      {% if IS_PRODUCTION %} 
        var element = document.createElement("script");
        //element.src = '{{ app.request.basepath }}/js/dist/parts/{{partName}}-{{ filesSignature }}.js';
        element.src = '{{ app.request.basepath }}/js/dist/parts/{{partName}}{{ filesSignature }}.js';
        element.onload = function(){
          {% if app.dtToken is defined %}
            gmjs.init('{{app.dtToken}}', false, {{mapFramework}});
          {% else %}
            gmjs.init(undefined, false, {{mapFramework}});
          {% endif %}
          gmjs.controller.init();
        }
        document.body.appendChild(element);
      {% else %}  
        {% if app.dtToken is defined %}
          gmjs.init('{{app.dtToken}}', true, {{mapFramework}});
        {% else %}
          gmjs.init(undefined, true, {{mapFramework}});
        {% endif %}
        gmjs.controller.init();
      {% endif %}
    });
  </script>
{% endif %}