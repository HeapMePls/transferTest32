{% extends ('layout.html.twig') %}

{% set isMobile = isMobile() %}

{% block admConfigHead %} 
    {% if (not isAdultContent) %}
      googletag.cmd.push(function() {
          {% if isMobile %}
          googletag.defineSlot('{{ADS_SLOT_TOP}}', [[320, 100],[300, 100],[320, 50]], 'sas_27839').addService(googletag.pubads());
          googletag.defineSlot('{{ADS_SLOT_MIDDLE}}', [[320, 100],[300, 100],[320, 50]], 'sas_28425').addService(googletag.pubads());
          googletag.defineSlot('{{ADS_SLOT_BOTTOM}}', [[320, 100],[300, 100],[320, 50]], 'sas_28427').addService(googletag.pubads());
          {% else %}
          googletag.defineSlot('{{ADS_SLOT_RIGHT}}', [[300,250]], 'sas_27528').addService(googletag.pubads());
          googletag.defineSlot('{{ADS_SLOT_RIGHT1}}', [[300,250]], 'sas_27838').addService(googletag.pubads());
          {% endif %}
          googletag.pubads().setTargeting('rubro', '{{ rubroNombre }}');
          googletag.pubads().setTargeting('zona', '{{ zonaNombre }}');
          googletag.pubads().setTargeting('Channel', 'web');
          googletag.pubads().enableSingleRequest();
          googletag.enableServices();
      });
    {% endif %}
{% endblock admConfigHead %}
{% block admConfigBody %} 
  {% if (not isAdultContent) %}
    <script>
        {% if isMobile %}
            googletag.cmd.push(function() { 
                googletag.display('sas_27839'); 
                googletag.display('sas_28425'); 
                googletag.display('sas_28427'); 
            });
        {% else %}
            googletag.cmd.push(function() { 
                googletag.display('sas_27528'); 
                googletag.display('sas_27838'); 
            });
        {% endif %}
    </script>
  {% endif %}
{% endblock admConfigBody %} 

{% block custom_stylesheets %}
    {% if IS_PRODUCTION %} 
        <style>
            {{ source( '_inlinecss/'~BRANDCODE~'/c-b'~filesSignature~'.css') }}
        </style>
    {% endif %}
{% endblock custom_stylesheets %}

{% block javascripts %}
  {# JAVASCRIPT - START #}
  {% include 'layout/jsloader.html.twig' with {'v':1} %}
  {# JAVASCRIPT - END #}
  {# JAVASCRIPT 2 TEST - START #}
  {# {% if not IS_PRODUCTION %} 
    <script src="{{ app.request.basepath }}/js/vendors/tarekautocomplete/autoComplete.min.js"></script>
    <script src="{{ app.request.basepath }}/js/vendors/storage/js.storage.min.js"></script>
    <script src="{{ app.request.basepath }}/js/vendors/lazysizes/lazysizes.min.js"></script>
    <script src="{{ app.request.basepath }}/js2/common/logger.js"></script>
    <script src="{{ app.request.basepath }}/js2/common/utils.js"></script>
    <script src="{{ app.request.basepath }}/js2/common/webpush.js"></script>
    <script src="{{ app.request.basepath }}/js2/common/worker.service.js"></script>
    <script src="{{ app.request.basepath }}/js2/common/reveal.js"></script>
    <script src="{{ app.request.basepath }}/js2/components/autocomplete.component.js"></script>
    <script src="{{ app.request.basepath }}/js2/config.js"></script>
    <script src="{{ app.request.basepath }}/js2/app.js"></script>
    <script src="{{ app.request.basepath }}/js2/pages/search.js"></script>
  {% endif %}
