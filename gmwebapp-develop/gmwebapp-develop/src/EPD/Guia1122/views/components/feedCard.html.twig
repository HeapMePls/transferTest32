      </div>
    {% endif %}
    <div style="padding: 10px;text-align:center">
      {% if post.zone != null and post.zone | length > 0 %}
        <h3 class="mpost-card-bigsubtitle">Nuevo negocio en {{post.zone}}!</h3>
      {% else %}
      <h3 class="mpost-card-bigsubtitle">Nuevo negocio!</h3>
      {% endif %}
      <p class="mpost-card-bigtitle">{{post.storeName}}</p>
    </div>
