<div class="mpost-card">
  <div class="mpost-card-head">
    {% if post.storeIconUrl is defined and post.storeIconUrl != NULL %}
      <picture >
          <source type="image/webp" data-srcset="{{post.storeIconUrl | asWebp}}">
          <source type="image/png" data-srcset="{{post.storeIconUrl}}">
          <img class="lazyload" data-src="{{post.storeIconUrl}}" alt="{{post.storeName}}"
              src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
      </picture>
    {% endif %}
    <div>
      <div class="mpost-card-head-title">
        <a href="{{path('comercio', {'id': 'LOC' ~ post.idrids, 'comercioNombre': urlize(post.storeName) }) }}" title="{{ post.storeName }}">
          {{post.storeName}}
        </a>
      </div>
      <div class="mpost-card-head-date">{{post.niceDate}}</div>
    </div>
  </div>
  {% if post.type == 'GALLERY' %}
    {% if post.image %}
      <a href="{{path('galeria', {'id': post.idrids, 'idg': post.entityId, 'comercioNombre': urlize(post.storeName)})  }}" title="{{post.title}}">
        <picture >
          <source type="image/webp" data-srcset="{{post.image | asWebp}}">
          <source type="image/png" data-srcset="{{post.image}}">
          <img class="lazyload" data-src="{{post.image}}" alt="{{post.title}}"
              src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
        </picture>
      </a>
    {% endif %}
    <div style="padding: 10px;">
    
      <h3><a href="{{path('galeria', {'id': post.idrids, 'idg': post.entityId, 'comercioNombre': urlize(post.storeName)})  }}" title="{{post.title}}">{{post.title}}</a></h3>
      <p>{{post.summary}}</p>
    </div>
    {# <div class="mpost-link">
      <a class="button clear" href="{{path('galeria', {'id': post.idrids, 'idg': post.entityId, 'comercioNombre': urlize(post.storeName)})  }}">Ver galer√≠a</a>
    </div> #}
  {% elseif post.type == 'VIDEO' %}
    {% if post.image %}
      <a href="{{path('video', {'id': post.idrids, 'idv': post.entityId, 'comercioNombre': urlize(post.storeName)})  }}" title="{{post.title}}">
        <img class="lazyload" data-src="{{post.image}}" alt="{{post.title}}">
      </a>
    {% endif %}
    <div style="padding: 10px;">
      <h3><a href="{{path('video', {'id': post.idrids, 'idv': post.entityId, 'comercioNombre': urlize(post.storeName)})  }}" title="Nuevo video de {{post.storeName}}">Nuevo video de {{post.storeName}}</a></h3>
      <p>{{post.title}}</p>
    </div>
    {# <div class="mpost-link">
      <a class="button clear" href="{{path('video', {'id': post.idrids, 'idv': post.entityId, 'comercioNombre': urlize(post.storeName)})  }}">Ver video</a>
    </div> #}
  {% elseif post.type == 'NEW_STORE' %}
    {% if post.storeImageUrl is defined and post.storeImageUrl != NULL and post.storeImageUrl | length > 0 %}
      <a href="{{path('comercio', {'id': 'LOC' ~ post.idrids, 'comercioNombre': urlize(post.storeName) }) }}" title="{{ post.storeName }}">
        <picture >
          <source type="image/webp" data-srcset="{{post.storeImageUrl | asWebp}}">
          <source type="image/png" data-srcset="{{post.storeImageUrl}}">
          <img class="lazyload" data-src="{{post.storeImageUrl}}" alt="{{post.storeName}} en {{post.zone}}"
