          </table>
        </div>
      {% endif %}
    </div>
  {% elseif post.type == 'COMMENT' %}
    <div class="mpost-card-cmt">
      {% if post.like == 1 %}
        {% if post.userName != null and post.userName | length > 0 %}
          <strong>{{post.userName}}</strong> ha recomendado a 1 <strong>{{post.storeName}}</strong>
        {% else %}
          <h3>Han recomendado a <strong>{{post.storeName}}</strong></h3>
        {% endif %}
      {% elseif post.like == 0 %}
        {% if post.userName != null and post.userName | length > 0 %}
          <h3><strong>{{post.userName}}</strong> ha opinado sobre <strong>{{post.storeName}}</strong></h3>
        {% else %}
          <h3>Han opinado sobre <strong>{{post.storeName}}</strong></h3>
        {% endif %}
      {% elseif post.stars != null %}\
        {% if post.stars > 3 %}
          {% if post.userName != null and post.userName | length > 0 %}
            <h3><strong>{{post.userName}}</strong> ha recomendado a 2 <strong>{{post.storeName}}</strong></h3>
          {% else %}
            <h3>Han recomendado a <strong>{{post.storeName}}</strong></h3>
          {% endif %}
        {% else %}
          {% if post.userName != null and post.userName | length > 0 %}
            <h3><strong>{{post.userName}}</strong> ha opinado sobre <strong>{{post.storeName}}</strong></h3>
          {% else %}
            <h3>Han opinado sobre <strong>{{post.storeName}}</strong></h3>
          {% endif %}
        {% endif %}
      {% else %}
        <h3>{{post.title}}</h3>
      {% endif %}
      <p class="mpost-card-cmttext">{{post.summary}}</p>
    </div>
  {% endif %}

  </div>