<?xml version="1.0" encoding="UTF-8" ?>
{{ headerXml | raw }}
{% if data|length>0 %}
  {% for parametro in data %}
    {#nodo apertura del xml#}
    {% if paramQ is empty %}
      <sitemap>
    {% else %}
      <url>
    {% endif %}
    {% if parametro.priority is defined %}
      <priority>{{parametro.priority}}</priority>
    {% endif %}
    {% if parametro.url is defined %}
      <loc>{{parametro.url}}</loc>
    {% endif %}
    {% if parametro.changefreq is defined %}
      <changefreq>{{parametro.changefreq}}</changefreq>
    {% endif %}
    {% if parametro.lastmod is defined %}
      <lastmod>{{parametro.lastmod}}</lastmod>
    {% endif %}
    {% if paramQ is empty %}
      </sitemap>
    {% else %}
      </url>
    {% endif %}
  {% endfor %}
{% endif %}
{{ footerXml | raw }}