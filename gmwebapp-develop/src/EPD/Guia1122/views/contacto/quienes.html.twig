{% extends ('layout.html.twig') %}

{% set isMobile = isMobile() %}

{% block custom_stylesheets %}
    {% if IS_PRODUCTION %} 
        <style>
            {{ source( '_inlinecss/'~BRANDCODE~'/c-h'~filesSignature~'.css') }}
        </style>
    {% endif %}
{% endblock custom_stylesheets %}

{% block pageTitle %}Quienes somos - Acerca de nosotros{% endblock pageTitle %}

{% block stylesheets %}
    {{ parent() }}
{% endblock stylesheets %}

{% block javascripts %}
  {# JAVASCRIPT - START #}
  {% include 'layout/jsloader.html.twig' with {'v':1} %}
  {# JAVASCRIPT - END #}
  {{ parent() }}
{% endblock javascripts %}

{% block metas %}
    <link rel="canonical" href="https:{{ url('quienes', {}, true) }}"/>
    {% if COUNTRY == 'uy' %}
      <meta name="description" content="GuíaMóvil 1122 es el directorio de negocios y profesionales líder en Uruguay. Nuestro servicio fue lanzado en 2008 y desde entonces hemos buscado evolucionar con nuestra gente, la tecnología y el país para brindar el mejor servicio para nuestros clientes." />
    {% else %}
      <meta name="description" content="BuscoInfo Paraguay es el directorio de negocios y profesionales líder en Paraguay. Nuestro servicio fue lanzado en 2008 y desde entonces hemos buscado evolucionar con nuestra gente, la tecnología y el país para brindar el mejor servicio para nuestros clientes." />
    {% endif %}
{% endblock metas %}

{% block header %}
    {{ render(path('headerComponent')) }}
{% endblock header %}

{% block content %}
    <div class="row u-box-cntr boxedlines">

        <div class="large-12 medium-12 small-12 abo-cover">

            {# COVER #}
            {% if isMobile %}
                <picture>
                    <source data-srcset="/img/about/covers/cover-m-3.webp" type="image/webp"/>
                    <source data-srcset="/img/about/covers/cover-m-3.jpg" type="image/png"/>
                    <img class="lazyload" data-src="/img/about/covers/cover-m-3.jpg"  
                        alt="Conectamos personas con negocios y profesionales en barrios de todo el país" 
                        src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
                </picture>
            {% else %}
                <picture>
                    <source data-srcset="/img/about/covers/cover-d-3.webp" type="image/webp"/>
                    <source data-srcset="/img/about/covers/cover-d-3.jpg" type="image/png"/>
                    <img class="lazyload" data-src="/img/about/covers/cover-d-3.jpg"  
                        alt="Conectamos personas con negocios y profesionales en barrios de todo el país" 
                        src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
                </picture>
            {% endif %}
            {# TAGLINE #}
            <div>
              <h1 style="font-size: 27px;color: #0072b9;text-transform: none;margin: 0px;float: none;padding: 0px 10px;">
                {% if COUNTRY == 'uy' %}
                  Conectamos personas con negocios y profesionales en barrios de todo el Uruguay a traves de nuestros servicios.
                {% else %}
                  Conectamos personas con negocios y profesionales en barrios de todo Paraguay a traves de nuestros servicios.
                {% endif %}
              </h1>
            </div>
        </div>
        
        <div class="large-12 medium-12 small-12 columns">

            {# SECTION ABOUT US #}
            <div class="row">
                <div class="large-12 medium-12 small-12 columns" style="margin: 35px 0px;text-align: center;">
                    <div class="content">
                        <p class="desarrollo">
                          {% if COUNTRY == 'uy' %}
                            {{BRAND}} es el directorio de negocios y profesionales l&iacute;der en Uruguay.
                            Nuestro servicio fue lanzado en 2008 y desde entonces hemos buscado evolucionar con nuestra gente, 
                            la tecnología y el país para brindar el mejor servicio para nuestros clientes.
                          {% else %}
                            {{BRAND}} es el directorio de negocios y profesionales l&iacute;der en Paraguay.
                            Nuestro servicio fue lanzado en 2008 y desde entonces hemos buscado evolucionar con nuestra gente, 
                            la tecnología y el país para brindar el mejor servicio para nuestros clientes.
                          {% endif %}
                        </p>
                    </div>
                </div>
            </div>

            {# 3 FACTS #}
            <div class="row">
                <div class="large-4 medium-4 small-12 columns" style="margin: 25px 0px;text-align: center;">
                    <div style="padding: 27px;">
                        <picture>
                            <source data-srcset="/img/about/trusted.webp" type="image/webp"/>
                            <source data-srcset="/img/about/trusted.png" type="image/png"/>
                            <img class="lazyload" data-src="/img/about/trusted.png"  
                                alt="Confianza" style="background-color: transparent;"
                                src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
                        </picture>
                    </div>
                    <div>
                        M&aacute;s de 7000 clientes confían en nosotros y m&aacute;s de 230.000 usuarios /mes los encuentran.
                    </div>
                </div>
                <div class="large-4 medium-4 small-12 columns" style="margin: 25px 0px;text-align: center;">
                    <div style="padding: 27px;">
                        <picture>
                            <source data-srcset="/img/about/complete.webp" type="image/webp"/>
                            <source data-srcset="/img/about/complete.png" type="image/png"/>
                            <img class="lazyload" data-src="/img/about/complete.png"  
                                alt="Completa" style="background-color: transparent;"
                                src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
                        </picture>
                    </div>
                    <div>
                      {% if COUNTRY == 'uy' %}
                        La gu&iacute;a mas completa y consultada del Uruguay
                      {% else %}
                        La gu&iacute;a mas completa y consultada del Paraguay
                      {% endif %}
                    </div>
                </div>
                <div class="large-4 medium-4 small-12 columns" style="margin: 25px 0px;text-align: center;">
                    <div style="padding: 27px;">
                        <picture>
                            <source data-srcset="/img/about/rocket.webp" type="image/webp"/>
                            <source data-srcset="/img/about/rocket.png" type="image/png"/>
                            <img class="lazyload" data-src="/img/about/rocket.png"  
                                alt="Apoyo a PYMEs" style="background-color: transparent;"
                                src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
                        </picture>
                    </div>
                    <div>
                        M&aacute;s de 10 años apoyando las pymes del pa&iacute;s
                    </div>
                </div>
            </div>
            
            {% if COUNTRY == 'uy' %}
              {# COVERAGE #}
              <div class="row" style="background-color: #F1F8E9;">
                  <div class="large-6 medium-6 small-12 columns abo-sec-box">
                      <picture>
                          <source data-srcset="/img/about/urumap3.webp" type="image/webp"/>
                          <source data-srcset="/img/about/urumap3.png" type="image/png"/>
                          <img class="lazyload" data-src="/img/about/urumap3.png"  
                              alt="Cobertura de todo el pa&iacute;s" 
                              style="background-color: transparent;"
                              src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
                      </picture>
                  </div>
                  <div class="large-6 medium-6 small-12 columns abo-sec-txt">
                      <div>
                          <h3>Cobertura de todo el pa&iacute;s</h3>
                          <p>
                              Nos enfocamos en darle a los Uruguayos la mejor informacion local en sus barrios,
                              ya sean urbanos, suburbanos o rurales. Somos el unico servicio que cubre largo y ancho del pais.
                          </p>
                      </div>
                  </div>
              </div>
            {% endif %}

            {# SECTION TECH  #}
            <div class="row" style="margin: 35px 5px;">
                {% if isMobile %}
                    <div class="large-6 medium-6 small-12 columns abo-sec-box" >
                        <picture>
                            <source data-srcset="/img/about/dna2.webp" type="image/webp"/>
                            <source data-srcset="/img/about/dna2.png" type="image/png"/>
                            <img class="lazyload" data-src="/img/about/dna2.png"  
                                alt="Evoluci&oacute;n constante" 
                                style="background-color: transparent;"
                                src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
                        </picture>
                    </div>
                    <div class="large-6 medium-6 small-12 columns abo-sec-txt" >
                        <h3>Evoluci&oacute;n constante</h3>
                        <p>
                            Estamos en constante evolucion de nuestros servicios para dar lo mejor a nuestros clientes y a nuestros usuarios.
                            Dar la informacion correcta en el momento correcto y presentarla de la mejor manera.
                        </p>
                    </div>
                {% else %}
                    <div class="large-6 medium-6 small-12 columns abo-sec-txt" >
                        <h3>Evoluci&oacute;n constante</h3>
                        <p>
                            Estamos en constante evolucion de nuestros servicios para dar lo mejor a nuestros clientes y a nuestros usuarios.
                            Dar la informacion correcta en el momento correcto y presentarla de la mejor manera.
                        </p>
                    </div>
                    <div class="large-6 medium-6 small-12 columns abo-sec-box" >
                        <picture>
                            <source data-srcset="/img/about/dna2.webp" type="image/webp"/>
                            <source data-srcset="/img/about/dna2.png" type="image/png"/>
                            <img class="lazyload" data-src="/img/about/dna2.png"  
                                alt="Evoluci&oacute;n constante" 
                                style="background-color: transparent;"
                                src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
                        </picture>
                    </div>
                {% endif %}
            </div>

            {# SECTION VERIFICATION #}
            <div class="row" style="background-color: #FFFDE7;">
                <div class="large-6 medium-6 small-12 columns abo-sec-box" >
                    <div style="margin: 10px 10px 30px 10px;">
                        <svg height="105" viewBox="0 -11 512.02797 512" width="105"><path d="m441.644531 247.59375-11.457031-32.554688-123.582031 123.585938c-12.074219 12.097656-28.160157 18.75-45.25 18.75-17.085938 0-33.171875-6.652344-45.246094-18.75l-90.667969-90.667969c-12.097656-12.074219-18.753906-28.160156-18.753906-45.246093 0-17.089844 6.65625-33.175782 18.753906-45.25 12.074219-12.09375 28.160156-18.75 45.246094-18.75 17.089844 0 33.175781 6.65625 45.25 18.75l45.417969 45.398437 121.6875-121.683594-8.195313-23.253906-3.773437-22.570313c-2.367188-14.464843-11.90625-26.453124-25.453125-32.066406-13.609375-5.652344-28.796875-3.902344-40.722656 4.652344l-59.285157 42.921875-59.949219-43.03125c-12.03125-8.488281-27.261718-10.175781-40.789062-4.5-13.4375 5.632813-22.890625 17.601563-25.257812 31.980469l-11.605469 72.253906-72.683594 12.074219c-14.441406 2.371093-26.429687 11.90625-32.0625 25.453125-5.632813 13.589844-3.882813 28.820312 4.671875 40.746094l42.902344 59.285156-43.03125 59.945312c-8.46875 12.03125-10.175782 27.285156-4.5 40.789063 5.652344 13.441406 17.597656 22.890625 31.976562 25.261719l72.257813 11.625 12.074219 72.660156c2.347656 14.421875 11.820312 26.390625 25.257812 32.023437 13.546875 5.671875 28.800781 3.988281 40.941406-4.609375l59.285156-42.921875 59.796876 42.921875c7.316406 5.226563 15.828124 7.914063 24.46875 7.914063 5.480468 0 10.984374-1.066407 16.253906-3.261719 13.570312-5.632812 23.085937-17.621094 25.453125-32.023438l11.625-72.253906 72.660156-12.074218c14.421875-2.371094 26.371094-11.820313 32.023437-25.261719 5.675782-13.503907 3.988282-28.757813-4.609374-40.9375zm0 0" fill="#ffc107"/><path d="m261.355469 314.710938c-5.460938 0-10.921875-2.089844-15.082031-6.25l-90.667969-90.667969c-8.339844-8.339844-8.339844-21.824219 0-30.164063 8.34375-8.34375 21.824219-8.34375 30.167969 0l75.582031 75.582032 214.25-214.25c8.34375-8.339844 21.824219-8.339844 30.167969 0 8.339843 8.339843 8.339843 21.824218 0 30.164062l-229.335938 229.335938