        <div id="sas_27839" class="gmad-top"></div>
      </div>
    </div>

    {# PRESUPUESTOS #}
    {% if data.quoters | length > 0 %}
      <div class="row homecolcntr">
        <div class="large-12 medium-12 small-12 columns text-center">
          <div class="section-header">Solicitá tu presupuesto sin costo</div>
          <div class="section-subheader">Enviá los detalles de tu proyecto o necesidad y obtené varios presupuestos</div>
          <div class="row align-stretch">
            <div class="homecolgrid" >
              {% for sto in data.quoters %}
                {% set cloc = (sto.idr * 10000) + sto.ids %}
                <div class="homecolbox">
                  {% if sto.ims | length == 0 %}
                      <picture >
                          <source type="image/webp" data-srcset="{{ app.request.basepath }}/img/{{BRANDNOIMS}}.webp">
                          <source type="image/png" data-srcset="{{ app.request.basepath }}/img/{{BRANDNOIMS}}.png">
                          <img class="lazyload"  data-src="{{ app.request.basepath }}/img/{{BRANDNOIMS}}.png" alt="{{sto.nam}}">
                      </picture>
                  {% else %}
                      <picture >
                          <source type="image/webp" data-srcset="{{sto.ims | asWebp}}">
                          <source type="image/png" data-srcset="{{sto.ims}}">
                          <img class="lazyload" data-src="{{sto.ims}}" alt="{{sto.nam}}">
                      </picture>
                  {% endif %}
                  <div style="padding:10px 10px 0px 10px">
                    <h4>{{sto.nam}}</h4>
                    <h5>{{sto.adr}}</h5>
                    <h6>{{sto.nmz}}</h6>
                    
                    <a class="button" style="margin-top:5px;"
                        href="{{ path('uberto/quoteRequest', {'nam': urlize(sto.nam), 'idr': sto.idr, 'ids': sto.ids}) }}" 
                        title="Solicitar presupuesto en {{sto.nam}}">
                        Solicitar presupuesto
                    </a>
                  </div>
                </div>
              {% endfor %}
            </div>

          </div>
        </div>
      </div>
    {% endif %}


        
