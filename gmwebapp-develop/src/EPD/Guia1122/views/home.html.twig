    {# COLLECTIONS #}
    {% if collections | length > 0 %}
      {% for card in collections %}
        {% if card.name == 'collection' and card.data.stores | length > 0 %}
          <div class="row homecolcntr" style="margin-bottom: 40px;">
            <div class="large-12 medium-12 small-12 small-collapse" >
              <h2>{{card.data.title}}</h2>
              <h3>{{card.data.subtitle}}</h3>
            </div>
            <div class="homecolgrid" >
              {% for sto in card.data.stores %}
                {% set cloc = (sto.idr * 10000) + sto.ids %}
                <a href="{{path('comercio', {'id': 'LOC' ~ cloc, 'comercioNombre': urlize(sto.nam)}) }}" class="homecolbox">
                  {% if sto.ims | length == 0 %}
                      <picture >
                          <source type="image/webp" data-srcset="{{ app.request.basepath }}/img/{{BRANDNOIMS}}.webp">
                          <source type="image/png" data-srcset="{{ app.request.basepath }}/img/{{BRANDNOIMS}}.png">
                          <img class="lazyload"  data-src="{{ app.request.basepath }}/img/{{BRANDNOIMS}}.png" alt="{{sto.nam}}">
                      </picture>
                  {% else %}
                      <picture >
                          <source type="image/webp" data-srcset="{{sto.ims | asWebp}}">
                          <source type="image/png" data-srcset="{{sto.ims}}">
                          <img class="lazyload" data-src="{{sto.ims}}" alt="{{sto.nam}}">
                      </picture>
                  {% endif %}
                  <div style="padding:10px">
                    <h4>{{sto.nam}}</h4>
                    <h5>{{sto.adr}}</h5>
                    <h6>{{sto.nmz}}</h6>
                  </div>
                </a>
              {% endfor %}
            </div>
          </div>
        {% endif %}

      {% endfor %}
    {% endif %}

    {# BANNER #}
    <div class="row ">
      <div class="large-12 medium-12 small-12 small-collapse text-center" style="margin: 20px 0px;">
        {% if isMobile %}
          <div id="sas_28427" class="gmad-top"></div>
        {% else %}
          <div id="sas_28427" class="gmad-top"></div>
        {% endif %}
      </div>
    </div>

    {# POSTS FEED #}
    {% if data.posts | length > 0 %}
      <div class="row " style="margin-top:40px">
        <div class="large-12 medium-12 small-12 columns">
          <div class="section-header text-center">Novedades</div>
          <div class="section-subheader text-center">Enterate de lo que esta pasando en tu barrio!</div>
          <div class="homeposts">

            {% for post in data.posts %}
              {% include 'components/feedCard.html.twig' with {'post': post} %}
            {% endfor %}

          </div>
          <div style="text-align:center;margin-top:20px;">
            <a class="button" style="margin-top:5px;"
                href="{{ url('novedades') }}" 
                title="Ver más novedades">Ver más novedades</a>
          </div>
        </div>
      </div>
    {% endif %}

