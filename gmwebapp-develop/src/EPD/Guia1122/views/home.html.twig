{% extends ('layout.html.twig') %}

{% set isMobile = isMobile() %}
{% set hasDynamicCovers = (data.covers | length > 0) %}

{# ADMANAGER CONFIG #}
{% block admConfigHead %} 
    googletag.cmd.push(function() {
        {% if isMobile %}
        googletag.defineSlot('{{ADS_SLOT_TOP}}', [[320, 100],[300, 100],[320, 50]], 'sas_27839').addService(googletag.pubads());
        googletag.defineSlot('{{ADS_SLOT_MIDDLE}}', [[320, 100],[300, 100],[320, 50]], 'sas_28427').addService(googletag.pubads());
        {% else %}
        googletag.defineSlot('{{ADS_SLOT_TOP}}', [[728,90],[970, 90]], 'sas_27839').addService(googletag.pubads());
        googletag.defineSlot('{{ADS_SLOT_MIDDLE}}', [[728,90],[970, 90]], 'sas_28427').addService(googletag.pubads());
        {% endif %}
        googletag.pubads().setTargeting('Channel', 'web');
        googletag.pubads().setTargeting('zona', '{{ lastzone }}');
        googletag.pubads().enableSingleRequest();
        googletag.enableServices();
    });
{% endblock admConfigHead %}
{% block admConfigBody %} 
    <script>
        {% if isMobile %}
            googletag.cmd.push(function() { 
                googletag.display('sas_27839'); 
                googletag.display('sas_28427'); 
            });
        {% else %}
            googletag.cmd.push(function() { 
                googletag.display('sas_27839'); 
                googletag.display('sas_28427'); 
            });
        {% endif %}
    </script>
{% endblock admConfigBody %} 


{% block custom_stylesheets %}
    {% if IS_PRODUCTION  %} 
        <style>
            {{ source( '_inlinecss/'~BRANDCODE~'/c-h'~filesSignature~'.css') }}
        </style>
    {% endif %}
{% endblock custom_stylesheets %}

{% block javascripts %}
  {# JAVASCRIPT - START #}
  {# {% include 'layout/jsloader.html.twig' with {'v':1} %} #}
  {# JAVASCRIPT - END #}
  {# JAVASCRIPT 2 TEST - START #}
  {% if not IS_PRODUCTION %} 
    <script src="{{ app.request.basepath }}/js/vendors/tarekautocomplete/autoComplete.min.js"></script>
    <script src="{{ app.request.basepath }}/js/vendors/storage/js.storage.min.js"></script>
    <script src="{{ app.request.basepath }}/js/vendors/lazysizes/lazysizes.min.js"></script>
    <script src="{{ app.request.basepath }}/js2/common/logger.js"></script>
    <script src="{{ app.request.basepath }}/js2/common/utils.js"></script>
    <script src="{{ app.request.basepath }}/js2/common/webpush.js"></script>
    <script src="{{ app.request.basepath }}/js2/common/worker.service.js"></script>
    <script src="{{ app.request.basepath }}/js2/common/reveal.js"></script>
    <script src="{{ app.request.basepath }}/js2/components/autocomplete.component.js"></script>
    <script src="{{ app.request.basepath }}/js2/config.js"></script>
    <script src="{{ app.request.basepath }}/js2/app.js"></script>
    <script src="{{ app.request.basepath }}/js2/pages/home.js"></script>
  {% endif %}
  {% include 'layout/jsloader.html.twig' with {'partName':'h', 'v':2} %}
  {# JAVASCRIPT 2 TEST - END #}
  {{ parent() }}
{% endblock javascripts %}

{% block pageTitle %}{{ parent() }}{% endblock pageTitle %}

{% block metas %}
    {{ parent() }}
    <link rel="canonical" href="https:{{ url('home', {}, true) }}"/>
    <link rel="preconnect" href="https://www.tingelmar.com/" />
    <link rel="dns-prefetch" href="https://www.tingelmar.com/" />
    <link rel="preconnect" href="https://static.tingelmar.com/" />
    <link rel="dns-prefetch" href="https://static.tingelmar.com/" />

    {% if (hasDynamicCovers) %}
      {% if isMobile %}
        <link rel="preload" as="image" href="{{data.covers[0].mobile}}" />
      {% else %}
        <link rel="preload" as="image" href="{{data.covers[0].desktop}}" />
      {% endif %}
    {% else %}
      <link rel="preload" as="image" href="{{ app.request.basepath }}/img/home/default-cover-3m.webp" />
      <link rel="preload" as="image" href="{{ app.request.basepath }}/img/home/default-cover-3m.jpg" />
    {% endif %}
{% endblock metas %}


{% block content %}

<div class="home">

  <div style="background-color: #FFFFFF;">
    {# HOME HEADER #}
    <div class="homehead">
      <div>
        <a href="{{ path('home') }}" title="Inicio" class="homeheadlogo">
          {% if COUNTRY == 'uy' %}
            <picture>
              <source type="image/webp" srcset="{{ app.request.basepath }}/img/logo_head5.webp?{{ assetVersion }}">
              <source type="image/jpeg" srcset="{{ app.request.basepath }}/img/logo_head5.png?{{ assetVersion }}">
              <img style="width:117px;height:37px"
                decoding="async" src="{{ app.request.basepath }}/img/logo_head5.png?{{ assetVersion }}" alt="1122">
            </picture>
          {% elseif COUNTRY == 'py' %}
            <picture>
              <source type="image/webp" srcset="{{ app.request.basepath }}/img/logobi_head.webp?{{ assetVersion }}">
              <source type="image/jpeg" srcset="{{ app.request.basepath }}/img/logobi_head.png?{{ assetVersion }}">
              <img style="width:117px;height:25px" decode="async" src="{{ app.request.basepath }}/img/logobi_head.png?{{ assetVersion }}" alt="BuscoInfo">
            </picture>
          {% endif %}
        </a>
        <div class="homeheadbtns">
          {# {% if COUNTRY == 'uy' %}
            <a href="https://empresas.1122.com.uy/" style="font-weight: 500;">Agregar mi negocio</a>
          {% else %}
            <a href="https://empresas.buscoinfo.com.py/" style="font-weight: 500;">Agregar mi negocio</a>
          {% endif %} #}
          <a href="/login?retUrl=/" style="color: #666;">Iniciar sesión</a>
        </div>
      </div>
    </div>

    {# COVER #}
    <div class="homecover">
      {% if not hasDynamicCovers %}
          {% if isMobile %}
            <picture >
              <source type="image/webp" srcset="{{ app.request.basepath }}/img/home/default-cover-3m.webp">
              <source type="image/jpg" srcset="{{ app.request.basepath }}/img/home/default-cover-3m.jpg">
              <img decoding="async" src="{{ app.request.basepath }}/img/home/default-cover-3m.jpg" 
                  alt="{{BRAND}}" title="{{BRAND}}"
                  src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
            </picture>
          {% else %}
            <picture >
                <source type="image/webp" srcset="{{ app.request.basepath }}/img/home/default-cover-3.webp">
                <source type="image/jpg" srcset="{{ app.request.basepath }}/img/home/default-cover-3.jpg">
                <img decoding="async" src="{{ app.request.basepath }}/img/home/default-cover-3.jpg" 
                    alt="{{BRAND}}" title="{{BRAND}}"
                    src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==">
            </picture>
          {% endif %}
      {% else %}
          {% if data.covers[0].actionUrl | length > 0 %}
              <a href="{{data.covers[0].actionUrl}}">
                  {% if isMobile %}
                      <picture>
                          <source srcset="{{data.covers[0].mobile | asWebp }}" type="image/webp"/>
                          <source srcset="{{data.covers[0].mobile}}" type="image/png"/>
                          <img decoding="async" class="cover-img" src="{{data.covers[0].mobile}}" 
                              alt="{{data.covers[0].title}} en {{BRAND}}">
                      </picture>
                  {% else %}
                      <picture>
                          <source srcset="{{data.covers[0].desktop | asWebp }}" type="image/webp"/>
                          <source srcset="{{data.covers[0].desktop}}" type="image/png"/>
                          <img decoding="async" class="cover-img" src="{{data.covers[0].desktop}}"
                              alt="{{data.covers[0].title}} en {{BRAND}}">
                      </picture>
                  {% endif %}
              </a>
          {% else %}
              {% if isMobile %}
                  <picture>
                      <source srcset="{{data.covers[0].mobile | asWebp }}" type="image/webp"/>
                      <source srcset="{{data.covers[0].mobile}}" type="image/png"/>
                      <img decoding="async" class="cover-img" src="{{data.covers[0].mobile}}" 
                          alt="{{data.covers[0].title}} en {{BRAND}}">
                  </picture>
              {% else %}
                  <picture>
                      <source srcset="{{data.covers[0].desktop | asWebp }}" type="image/webp"/>
                      <source srcset="{{data.covers[0].desktop}}" type="image/png"/>
                      <img decoding="async" class="cdata-over-img" src="{{data.covers[0].desktop}}"
                          alt="{{data.covers[0].title}} en {{BRAND}}">
                  </picture>
              {% endif %}
          {% endif %}
      {% endif %}
    </div>

    {# SEARCHBOX #}
    <div class="row " style="padding: 30px 0px;background-color: white;">
      <div class="large-12 medium-12 small-12">
        <h1 class="section-header" style="margin-bottom: 25px;">
          Encontrá los mejores lugares y profesionales de tu zona
        </h1>
      </div>
      <div class="large-12 medium-12 small-12" style="padding: 10px;">
        {% include 'components/search2.html.twig' with {'showAboveButtons':false} %}
      </div>
    </div>

    {# MAIN CATEGORIES #}
    <div class="row " style="margin-top:20px;padding: 10px 0px 20px 0px;">
