{% extends ('layout.html.twig') %}

{% set isMobile = isMobile() %}
{% block admConfigHead %} 
    googletag.cmd.push(function() {
        {% if isMobile %}
        googletag.defineSlot('{{ADS_SLOT_MIDDLE}}', [[320, 100],[300, 100],[320, 50]], 'sas_28425').addService(googletag.pubads());
        googletag.defineSlot('{{ADS_SLOT_BOTTOM}}', [[320, 100],[300, 100],[320, 50]], 'sas_28427').addService(googletag.pubads());
        {% else %}
        googletag.defineSlot('{{ADS_SLOT_TOP}}', [[300,250]], 'sas_27528').addService(googletag.pubads());
        googletag.defineSlot('{{ADS_SLOT_MIDDLE}}', [[300,250]], 'sas_27838').addService(googletag.pubads());
        {% endif %}
        googletag.pubads().setTargeting('Channel', 'web');
        googletag.pubads().setTargeting('zona', '{{ lastzone }}');
        googletag.pubads().enableSingleRequest();
        googletag.enableServices();
    });
{% endblock admConfigHead %}
{% block admConfigBody %} 
    <script>
        {% if isMobile %}
            googletag.cmd.push(function() { 
                googletag.display('sas_28425'); 
                googletag.display('sas_28427'); 
            });
        {% else %}
            googletag.cmd.push(function() { 
                googletag.display('sas_27528'); 
                googletag.display('sas_27838'); 
            });
        {% endif %}
    </script>
{% endblock admConfigBody %} 

{% block custom_stylesheets %}
    {% if IS_PRODUCTION %} 
        <style>
            {{ source( '_inlinecss/'~BRANDCODE~'/c-a'~filesSignature~'.css') }}
        </style>
    {% endif %}
{% endblock custom_stylesheets %}

{% block pageTitle %} Resultados de juegos de azar | {{ parent() }}{% endblock pageTitle %}

{% block metas %}
    <link rel="canonical" href="https:{{ url('azar', {}, true) }}"/>
    <meta name="description" content="Conoce los ultimos resultados de los juegos de azar como 5 de oro, tombola, quiniela y la loteria uruguaya | {{BRAND}}" />
    <meta name="keywords" content="juegos de azar, 5 de oro, tombola, quiniela, loteria, comercios, uruguay, servicios" />
    <meta name="robots" content="index, all" />
    <meta name="DC.title" content="{{BRAND}}" />
    <meta name="publisher" content="{{BRAND}}" />
    <meta name="language" content="es" />
    <meta name="distribution" content="global"/>
{% endblock metas %}

